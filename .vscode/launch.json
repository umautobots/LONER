  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "LonerSLAM: Fusion Portable",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/examples/run_loner.py",
      "console": "integratedTerminal",
      "args": ["${workspaceFolder}/cfg/fusion_portable/garden.yaml"],
      "cwd": "${workspaceFolder}/examples",
      "subProcess": true
    },
    {
      "name": "LonerSLAM: Newer College",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/examples/run_loner.py",
      "console": "integratedTerminal",
      "args": ["${workspaceFolder}/cfg/newer_college/quad.yaml"],
      "cwd": "${workspaceFolder}/examples",
      "subProcess": true
    }
  ]
}