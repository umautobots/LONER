  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "ClonerSLAM: Fusion Portable",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/examples/fusion_portable/run_fusion_portable.py",
      "console": "integratedTerminal",
      "args": ["${workspaceFolder}/cfg/fusion_portable/canteen.yaml"],
      "cwd": "${workspaceFolder}/examples/fusion_portable",
      "subProcess": true
    }
  ]
}